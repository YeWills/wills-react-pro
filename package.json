{
  "name": "wills-react-pro",
  "version": "0.0.1",
  "description": "Complete Enterprise Front End Project with react",
  "main": "src/index.js",
  "scripts": {
    "start": "concurrently 'npm run mock:server' 'npm run dev'",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress --colors",
    "mock:server": "cross-env json-server --watch db/db.json --middlewares middleware/loginMiddleware.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack -p --progress --colors",
    "clean": "rm -rf build/",
    "lint": "eslint src",
    "test": "jest",
    "test-cov": "jest --coverage",
    "test:watch": "jest --watch --coverage"
  },
  "author": "YeWills",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.0.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.1",
    "json-server": "^0.14.0",
    "less": "^3.7.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^2.1.6",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "@react-hero/form": "^0.1.0",
    "add": "^2.0.6",
    "antd": "^3.7.1",
    "axios": "^0.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.1",
    "connected-react-router": "^4.3.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "history": "^4.6.3",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.10",
    "path-to-regexp": "^2.2.1",
    "prop-types": "^15.6.2",
    "react": "^16.13.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "4.8.4",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "react-sider": "^0.3.4",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetTransformer.js",
      "\\.(css|less|scss)$": "<rootDir>/assetTransformer.js"
    },
    "resetMocks": true,
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
